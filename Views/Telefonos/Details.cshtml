@model personapi_dotnet.Models.Entities.Telefono

<h1>Detalles del Teléfono</h1>

<div>
    <h4>Teléfono</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Num)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Num)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Oper)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Oper)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Duenio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Duenio)
        </dd>
    </dl>

    <a asp-action="Edit" asp-route-num="@Model.Num" class="btn btn-primary">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>
