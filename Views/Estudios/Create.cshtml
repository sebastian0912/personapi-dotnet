@model personapi_dotnet.Models.Entities.Estudio

<h2>Create</h2>

<h4>Estudio</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="IdProf" class="control-label"></label>
                <input asp-for="IdProf" class="form-control" />
                <span asp-validation-for="IdProf" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CcPer" class="control-label"></label>
                <input asp-for="CcPer" class="form-control" />
                <span asp-validation-for="CcPer" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fecha" class="control-label"></label>
                <input asp-for="Fecha" type="date" class="form-control" />
                <span asp-validation-for="Fecha" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Univer" class="control-label"></label>
                <input asp-for="Univer" class="form-control" />
                <span asp-validation-for="Univer" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
        <div asp-validation-summary="All" class="text-danger"></div>

    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
